	//Programa
	Escribir "Ingrese Numero: "
	Leer n
	
	Para i<-1 Hasta 6 Con Paso 1 Hacer
		m <- n * i
		Escribir n, " x ", i, " = ", m
	Fin Para
	
FinAlgoritmo
